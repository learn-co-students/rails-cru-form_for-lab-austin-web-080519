

<h1>Songs</h1>
<% @songs.each do |song|%>

<p>My Song: <%=link_to song.name, song_path(song)%></p>

<p><%= link_to "My Artist:", artists_path%>

<%= link_to song.artist.name, artist_path(song.artist)%></p><br>

<p><%= link_to "My Genre:", genres_path%>

 <%= link_to song.genre.name, genre_path(song.genre)%></p>

<% end %>